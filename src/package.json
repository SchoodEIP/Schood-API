{
  "name": "schood-api",
  "version": "1.0.0",
  "description": "Schood API",
  "main": "index.js",
  "jest": {
    "testPathIgnorePatterns": [
      "__tests__/serverUtils"
    ],
    "coveragePathIgnorePatterns": [
      "services/mailer"
    ],
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  },
  "scripts": {
    "start": "nodemon",
    "coverage": "jest --runInBand --testTimeout=15000 --coverage",
    "test": "jest --runInBand --testTimeout=15000",
    "linter": "standard --fix --global it --global expect --global describe --global beforeAll --global afterEach --global beforeEach --global afterAll"
  },
  "author": "Exiels",
  "license": "",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.1.1",
    "express": "^4.18.2",
    "jest": "^29.5.0",
    "joi": "^17.9.2",
    "joi-objectid": "^4.0.2",
    "jsonwebtoken": "^9.0.0",
    "mongo-sanitize": "^1.1.0",
    "mongoose": "^7.2.2",
    "nodemailer": "^6.9.2",
    "nodemon": "^2.0.22",
    "random-string-generator": "^1.0.1",
    "standard": "^17.1.0",
    "supertest": "^6.3.3",
    "swagger-ui-express": "^4.6.3",
    "yamljs": "^0.3.0"
  }
