{
  "name": "schood-api",
  "version": "1.0.0",
  "description": "Schood API",
  "main": "index.js",
  "jest": {
    "testPathIgnorePatterns": [
      "__tests__/serverUtils"
    ],
    "coveragePathIgnorePatterns": [
      "services/mailer",
      "config/db.default"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!my-package)(.*)"
    ],
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  },
  "scripts": {
    "start": "nodemon --ignore node_modules",
    "coverage": "jest --runInBand --testTimeout=15000 --coverage",
    "test": "jest --runInBand --testTimeout=15000",
    "linter": "standard --fix --global it --global expect --global describe --global beforeAll --global afterEach --global beforeEach --global afterAll"
  },
  "author": "Exiels",
  "license": "",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.0",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.0.2",
    "FormData": "^0.10.1",
    "jest": "^29.7.0",
    "joi": "^17.10.2",
    "joi-objectid": "^4.0.2",
    "jsonwebtoken": "^9.0.2",
    "mongo-sanitize": "^1.1.0",
    "mongoose": "^7.5.3",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.5",
    "nodemon": "^3.0.1",
    "random-string-generator": "^1.0.1",
    "semver": "^7.5.4",
    "standard": "^17.1.0",
    "supertest": "^6.3.3",
    "swagger-ui-express": "^5.0.0",
    "yamljs": "^0.3.0"
  },
  "nodemonConfig": {
    "legacyWatch": true
  },
  "paths": {
    "@/*": ["src/*"]
  }
}
