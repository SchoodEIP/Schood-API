{
  "name": "schood-api",
  "version": "1.0.0",
  "description": "Schood API",
  "main": "index.js",
  "jest": {
    "testPathIgnorePatterns": [
      "__tests__/serverUtils",
      "__tests__/fixtures"
    ],
    "coveragePathIgnorePatterns": [
      "__tests__/serverUtils",
      "__tests__/fixtures",
      "services/",
      "config/db.default",
      "config/db.config",
      "config/default/",
      "routes/user/modifyProfile",
      "models/"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!my-package)(.*)"
    ],
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  },
  "scripts": {
    "start": "nodemon --ignore node_modules",
    "coverage": "jest --runInBand --testTimeout=15000 --coverage",
    "test": "jest --runInBand --testTimeout=15000",
    "linter": "standard --fix --global it --global expect --global describe --global beforeAll --global afterEach --global beforeEach --global afterAll"
  },
  "author": "Exiels",
  "license": "",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.2.0",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.6",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "FormData": "^0.10.1",
    "jest": "^29.7.0",
    "joi": "^17.13.1",
    "joi-objectid": "^4.0.2",
    "jsonwebtoken": "^9.0.2",
    "mongo-sanitize": "^1.1.0",
    "mongoose": "^8.5.4",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.13",
    "nodemon": "^3.1.4",
    "random-string-generator": "^1.0.1",
    "semver": "^7.6.2",
    "standard": "^17.1.0",
    "supertest": "^7.0.0",
    "swagger-ui-express": "^5.0.1",
    "ws": "^8.18.0",
    "yamljs": "^0.3.0"
  },
  "nodemonConfig": {
    "legacyWatch": true
  },
  "paths": {
    "@/*": [
      "src/*"
    ]
  }
}
